        $(document).ready(function() {
            $("#myChart").hide();
            pc = new PointChart("#myChart", [10,20,30]);
            pc.add([10,20,30], "#ff0", "ico1");
            pc.add([20,30,40], "#0ff", "ico2");
            pc.draw();
            $("#myChart").show();
            tmp = new PointChart("#yourChart", [1,2,3,4,5]);
            tmp.add([1,2,3,4,5], "#111", "abc");
            tmp.add([2,3,4,5.11111112,6.18], "#4F4", "rj");
            tmp.draw();
            $( "#progressbar" ).progressbar({
                  value: 37,
                    max:100,
                    complete:function(event, ui) {
                                alert("finish");
                            }
            });
            $("#start").button().click(function(event){
                alert("start");
            });
        });
    </script>
